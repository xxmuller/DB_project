<AnchorPane prefHeight="850.0" prefWidth="1250.0" style="-fx-background-color: #232d40;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoginController">
   <AnchorPane prefHeight="850.0" prefWidth="625.0" style="-fx-background-color: #64d2c8;">
      <children>
         <ImageView fitHeight="372.0" fitWidth="371.0" layoutX="126.0" layoutY="175.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../icons/hotel_icon.png" />
            </image>
         </ImageView>
         <HBox layoutX="126.0" layoutY="572.0" prefHeight="35.0" prefWidth="362.0" style="-fx-background-color: transparent; -fx-border-color: white; -fx-border-width: 0PX 0PX 1.5PX 0PX;" />
         <HBox layoutX="176.0" layoutY="611.0" prefHeight="36.0" prefWidth="278.0" style="-fx-background-color: transparent; -fx-border-color: white; -fx-border-width: 0PX 0PX 1.5PX 0PX;" />
      </children></AnchorPane>
    <HBox layoutX="781.0" layoutY="434.0" prefHeight="50.0" prefWidth="343.0" style="-fx-background-color: transparent; -fx-border-color: white; -fx-border-width: 0px 0px 1.5px 0px;">
      <children>
            <PasswordField fx:id="textFieldPassword" prefHeight="34.0" prefWidth="251.0" promptText="**********" style="-fx-background-color: transparent; -fx-text-fill: white; -fx-prompt-text-fill: white;">
